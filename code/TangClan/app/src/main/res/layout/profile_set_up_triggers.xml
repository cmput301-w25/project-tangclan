<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF">


    <TextView
        android:id="@+id/set_up_your"
        android:layout_width="301dp"
        android:layout_height="60dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:gravity="center_horizontal|top"
        android:text="@string/set_up_your"
        android:textAppearance="@style/set_up_your"
        tools:layout_editor_absoluteX="55dp"
        tools:layout_editor_absoluteY="14dp" />

    <TextView
        android:id="@+id/triggers_ar"
        android:layout_width="340dp"
        android:layout_height="135dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:gravity="center_horizontal|top"
        android:text="@string/triggers_ar"
        android:textAppearance="@style/triggers_ar"
        tools:layout_editor_absoluteX="35dp"
        tools:layout_editor_absoluteY="74dp" />

    <TextView
        android:id="@+id/add_up_to_t"
        android:layout_width="328dp"
        android:layout_height="52dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:gravity="center_horizontal|top"
        android:text="@string/add_up_to_t"
        android:textAppearance="@style/add_up_to_t"
        tools:layout_editor_absoluteX="41dp"
        tools:layout_editor_absoluteY="181dp" />


    <RelativeLayout
        android:layout_width="265dp"
        android:layout_height="48dp"
        android:background="#F4EFE6"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.357">

    </RelativeLayout>

    <com.google.android.material.chip.ChipGroup
        android:id="@+id/chip_group"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="16dp"
        app:chipSpacing="8dp"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="297dp">

        <com.google.android.material.chip.Chip
            android:id="@+id/chip_dog"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="a dog"
            app:chipBackgroundColor="#F4EFE6"
            app:closeIcon="@drawable/baseline_cancel_24"
            app:closeIconEnabled="true" />

        <com.google.android.material.chip.Chip
            android:id="@+id/chip_coraline"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="the movie coraline"
            app:chipBackgroundColor="#E3F2FD"
            app:closeIcon="@drawable/baseline_cancel_24"
            app:closeIconEnabled="true" />

        <com.google.android.material.chip.Chip
            android:id="@+id/chip_smell"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="burning smell"
            app:chipBackgroundColor="#E3F2FD"
            app:closeIcon="@drawable/baseline_cancel_24"
            app:closeIconEnabled="true" />
    </com.google.android.material.chip.ChipGroup>

    <Button
        android:id="@+id/button_veri"
        android:layout_width="100dp"
        android:layout_height="38dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:clipToOutline="true"
        android:text="Next"
        tools:layout_editor_absoluteX="290dp"
        tools:layout_editor_absoluteY="420dp" />

    <TextView
        android:id="@+id/step_3_of_4"
        android:layout_width="169dp"
        android:layout_height="41dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:gravity="center_horizontal|top"
        android:text="@string/step_3_of_4"
        android:textAppearance="@style/step_3_of_4"
        tools:layout_editor_absoluteX="121dp"
        tools:layout_editor_absoluteY="458dp" />

    <EditText
        android:id="@+id/input_field"
        android:layout_width="265dp"
        android:layout_height="48dp"
        android:background="#F4EFE6"
        android:gravity="center_vertical"
        android:hint="Enter text"
        android:paddingStart="24dp"
        android:paddingEnd="48dp"
        android:text="Input"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.357" />

    <ImageView
        android:id="@+id/add_icon"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:src="@drawable/baseline_add_circle_outline_24"
        tools:layout_editor_absoluteX="297dp"
        tools:layout_editor_absoluteY="253dp" />

    <View
        android:id="@+id/untitled_31"
        android:layout_width="387dp"
        android:layout_height="416dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:background="@drawable/untitled_35"
        tools:layout_editor_absoluteX="17dp"
        tools:layout_editor_absoluteY="451dp" />


</androidx.constraintlayout.widget.ConstraintLayout>